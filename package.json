{
  "name": "earth-ui",
  "version": "0.3.1",
  "author": "Kimi Gao",
  "description": "A minimalism-style ui component library",
  "keywords": [
    "react.js",
    "component",
    "ui"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": "https://github.com/G-Explorer/earth-ui",
  "homepage": "https://ui.muwenzi.com/",
  "engines": {
    "node": ">=8.9.0",
    "npm": ">=5.2.0"
  },
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "lib": "rimraf lib && NODE_ENV=production babel src -d lib --ignore __tests__ && gulp --gulpfile config/gulpfile.js",
    "dist": "rimraf dist && webpack --progress --config config/webpack.config.js",
    "site:dist": "rimraf site/dist && SITE_ENV=production webpack --progress --config config/webpack.config.site.js",
    "site:dev": "webpack --progress --config config/webpack.config.site.js && node-dev site/server.js",
    "presite:prod": "npm i && npm run site:dist",
    "site:prod": "SITE_ENV=production pm2 start site/server.js --name earth-ui",
    "create": "node bin/create.js",
    "lint": "eslint src && eslint site",
    "fix": "eslint src --fix eslint site --fix",
    "test": "jest --config=config/jest.config.json",
    "coverage": "coveralls",
    "commit": "git-cz",
    "release": "npm run lib && npm run dist && standard-version"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "postcommit": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "browserslist": "defaults",
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0",
    "prop-types": ">=15.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "classlist": "^1.0.1",
    "classnames": "^2.2.5",
    "focus-trap": "^3.0.0",
    "invariant": "^2.2.2",
    "marked": "^0.5.2",
    "react-avatar-editor": "^10.3.0",
    "react-update": "^0.4.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@reach/router": "^1.1.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "commitizen": "^2.9.6",
    "compression": "^1.7.2",
    "css-loader": "^0.28.7",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.9.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-react": "^7.11.1",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.10",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "gulp": "^3.9.1",
    "gulp-replace": "^1.0.0",
    "html-loader": "^0.5.5",
    "husky": "^0.15.0-rc.8",
    "jest": "^22.4.2",
    "jest-css-modules": "^1.1.0",
    "jest-environment-jsdom-global": "^1.0.3",
    "jsx-loader": "^0.13.2",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "lint-staged": "^7.0.0",
    "markdown-loader": "^2.0.2",
    "multiparty": "^4.1.3",
    "node-dev": "^3.1.3",
    "nprogress": "^0.2.0",
    "pm2": "^2.10.1",
    "postcss-loader": "^2.0.8",
    "prop-types": "^15.6.1",
    "rc-queue-anim": "^1.6.10",
    "rc-scroll-anim": "^2.5.6",
    "react": "^16.6.3",
    "react-copy-to-clipboard": "^5.0.1",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.6.3",
    "react-hot-loader": "^4.3.12",
    "react-imported-component": "^5.2.3",
    "rimraf": "^2.6.2",
    "standard": "^12.0.1",
    "standard-version": "^4.3.0",
    "style-loader": "^0.19.0",
    "styled-components": "^4.1.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.12.0",
    "webpack-dashboard": "^1.1.1",
    "webpack-dev-middleware": "^2.0.6",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-livereload-plugin": "^1.0.0",
    "webpack-merge": "^4.1.2"
  }
}
